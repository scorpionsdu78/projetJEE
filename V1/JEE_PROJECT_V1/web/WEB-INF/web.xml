<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <context-param>
        <param-name>username</param-name>
        <param-value>PWD2</param-value>
    </context-param>
    <servlet>
        <servlet-name>Employee_get</servlet-name>
        <servlet-class>fr.efrei.API.Employee_get</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Employee_get</servlet-name>
        <url-pattern>/Employee_get</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
